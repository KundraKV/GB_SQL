
 */
 База хранит: 
	перечень изготавливаемых продуктов, разделенных на продуктовые группы, а также их характеристики;
	данные производства, которые включают в себя не только продукт, а также цех изготовления и работников по должностям;
	данные склада, отражающие уровень минимальных и максимальных запасов, а также реальный остаток на складе.
Задачи БД:
	Хранение и обновление информации об изготавливаемых продуктах и их складских запасах.
  /*
  
  -- Создание БД
drop database production;
create database if not exists production;
use production;

-- Создание таблиц

drop table product;
create table if not exists product (
	id INT UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY COMMENT "Идентификатор строки",
  	name VARCHAR(255) NOT NULL COMMENT "Наименование продукта",
  	group_id INT UNSIGNED NOT null COMMENT "Ссылка на товарную группу"

);

drop table characteristic;
create table if not exists characteristic (
	product_id INT UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY COMMENT "Идентификатор строки",
	size_product_id INT UNSIGNED NOT NULL COMMENT "Ссылка на объем",
	units_id INT UNSIGNED NOT NULL COMMENT "Ссылка на единицу измерения",
	perfume_id INT UNSIGNED NOT NULL COMMENT "Ссылка на отдушку"
	);

drop table size_product;
create table if not exists size_product (
	id INT UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY COMMENT "Идентификатор строки",
	size_product VARCHAR(20) NOT NULL COMMENT "Объем"
);

drop table perfume;
create table if not exists perfume (
	id INT UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY COMMENT "Идентификатор строки",
	perfume_name VARCHAR(255) NOT NULL COMMENT "Назначение"
);

drop table units;
create table if not exists units (
	id INT UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY COMMENT "Идентификатор строки",
	units_name VARCHAR(255) NOT NULL COMMENT "Еденица измерения",
	symbol VARCHAR(10) NOT NULL COMMENT "Обозначение"
);

drop table product_group;
create table if not exists product_group (
	id INT UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY COMMENT "Идентификатор строки",
	group_name VARCHAR(255) NOT NULL unique COMMENT "Товарная группа"
);



drop table production;
create table if not exists production (
	id INT UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY COMMENT "Идентификатор строки",
	product_id INT UNSIGNED NOT NULL COMMENT "Ссылка на продукт",
	workshop_id INT UNSIGNED NOT NULL COMMENT "Ссылка на цех",
	product_new INT UNSIGNED NOT NULL COMMENT "Количество созданного продукта",
	created_at DATETIME DEFAULT CURRENT_TIMESTAMP COMMENT "Время создания"
);

drop table storage;
create table if not exists storage (
	product_id INT UNSIGNED NOT NULL COMMENT "Ссылка на продукт",
	quantity INT UNSIGNED NOT NULL COMMENT "Количество",
	quantity_max INT UNSIGNED NOT NULL COMMENT "Максимальный запас товара",
	quantity_min INT UNSIGNED NOT NULL COMMENT "Минимальный запас товара"
);

drop table worker;
create table if not exists worker (
	id INT UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY COMMENT "Идентификатор строки",
	skill VARCHAR(255) NOT NULL  COMMENT "Должность"
);

drop table workshop;
create table if not exists workshop (
	id INT UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY COMMENT "Идентификатор строки",
	workshop_name VARCHAR(255) NOT NULL COMMENT "Название"
);

create table if not exists making (
	production_id INT UNSIGNED NOT NULL COMMENT "Ссылка на производство",
	worker_id INT UNSIGNED NOT NULL COMMENT "Ссылка на работника"
);
	
	

-- Объеденение таблиц
alter table characteristic 
	add constraint characteristic_product_id_fk
		foreign key (product_id) references product (id);
	
alter table characteristic 
	add constraint characteristic_size_product_id_fk
		foreign key (size_product_id) references size_product (id),
	add constraint characteristic_units_id_fk
		foreign key (units_id) references units (id),
	add constraint characteristic_perfume_id_fk
		foreign key (perfume_id) references perfume (id);
	
 alter table product 
	 add constraint product_product_group_id_fk
		 foreign key (product_group_id) references product_group (id);

	
ALTER TABLE product DROP FOREIGN key product_product_group_id_fk;
ALTER TABLE product DROP FOREIGN key product_characteristic_id_fk;	
	

	
alter table production 
	add constraint production_product_id_fk
		foreign key (product_id) references product (id),
	add constraint production_workshop_id_fk
		foreign key (workshop_id) references workshop (id);
	
	
alter table storage 
	add constraint storage_product_id_fk
		foreign key (product_id) references product (id);


alter table making 
	add constraint making_production_id_fk
		foreign key (production_id) references production (id),
	add constraint making_worker_id_fk
		foreign key (worker_id) references worker (id);
	
	-- Заполнение таблиц

